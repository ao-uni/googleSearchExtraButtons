  } ],
  "converted_from_user_script": true,
  "description": "Add buttons (last 1/2/3 days, weeks, PDF search etc.) for results of Google search page",
  "name": "GoogleSearchExtraButtons",
  "version": "10.2015.11.30",
  "manifest_version": 2
}
