{
  "content_scripts": [ {
    "exclude_matches": [  ],
    "include_globs": [
      "http://www.google.*/search*"
    ,"https://www.google.*/search*"
    ,"https://encrypted.google.*/search*"
    ,"https://spmbt.github.io/googleSearchExtraButtons/saveYourLocalStorage.html"],
    "js": [ "Google_Search_Extra_Buttons.user.js" ],
    "matches": [ "http://*/*", "https://*/*" ],
